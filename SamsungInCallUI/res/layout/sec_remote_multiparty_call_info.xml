                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:id="@id/labelAndNumber" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@id/twophoneAndName">
                        <TextView android:textSize="@dimen/sec_call_label_text_size" android:textColor="@color/sec_incall_call_banner_default_subtext_color" android:id="@id/label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:fontFamily="sans-serif" android:layout_marginEnd="@dimen/sec_call_label_margin_right" />
