            <FrameLayout android:id="@id/secondaryPhotoContainer" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/sec_remote_multiparty_call_photo_top_margin" android:layout_marginStart="@dimen/sec_remote_multiparty_call_photo_left_margin">
                <ImageView android:id="@id/secondaryPhoto" android:background="@drawable/sec_photo_container_background_multiparty" android:layout_width="@dimen/sec_multiparty_call_photo_size" android:layout_height="@dimen/sec_multiparty_call_photo_size" android:scaleType="centerCrop" />
                <TextView android:textSize="@dimen/sec_multiparty_conference_text_size" android:textColor="@color/incall_call_conference_text_color" android:gravity="center" android:id="@id/secondaryPhotoText" android:background="@drawable/sec_photo_container_background_multiparty" android:layout_width="@dimen/sec_multiparty_call_photo_size" android:layout_height="@dimen/sec_multiparty_call_photo_size" android:fontFamily="sans-serif" />
            </FrameLayout>
            <RelativeLayout android:gravity="center_vertical" android:id="@id/multipartySecondaryCallInfo" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_marginStart="@dimen/sec_remote_multiparty_call_info_left_margin">
                <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@id/secondaryNameContainer" android:layout_width="wrap_content" android:layout_height="wrap_content">
