<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@id/primary_call_banner" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:id="@id/indicator_area" android:background="#00000000" android:layout_width="fill_parent" android:layout_height="@dimen/indicator_area_height" />
    <ViewStub android:id="@id/volte_addcall_stub" android:visibility="gone" android:layout="@layout/sec_volte_addcall_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@id/indicator_area" android:layout_alignWithParentIfMissing="true" android:layout_marginEnd="@dimen/sec_call_banner_side_padding" android:layout_alignParentEnd="true" />
    <RelativeLayout android:layout_gravity="center_vertical" android:id="@id/callStateContainer" android:clipChildren="false" android:clipToPadding="false" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/sec_call_state_layout_margin_top" android:layout_below="@id/indicator_area" android:paddingStart="@dimen/sec_call_banner_side_padding">
