                </LinearLayout>
                <ImageView android:id="@id/secondaryIcon" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/sec_multiparty_secondary_icon_margin_top" android:src="@drawable/call_hd_voice_square" android:scaleType="center" android:tint="@color/sec_incall_accent_default_color" android:layout_below="@id/secondaryNameContainer" android:importantForAccessibility="no" android:layout_marginEnd="@dimen/sec_uhd_icon_right_margin" />
                <LinearLayout android:id="@id/secondaryElapsedTimeLayout" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@id/secondaryNameContainer" android:importantForAccessibility="no" android:layout_toEndOf="@id/secondaryIcon">
                    <ViewStub android:id="@id/secondary_elapsed_time_stub" android:visibility="gone" android:layout="@layout/secondary_elapsed_time_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" android:layout_marginEnd="@dimen/sec_uhd_icon_right_margin" />
                </LinearLayout>
                <TextView android:textSize="@dimen/sec_call_label_text_size" android:textColor="@color/voicecall_multiparty_secondary_text_color" android:id="@id/secondaryLabel" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/onHold" android:layout_below="@id/secondaryNameContainer" android:importantForAccessibility="no" android:fontFamily="sans-serif" android:textDirection="ltr" android:layout_toEndOf="@id/secondaryElapsedTimeLayout" />
