<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/videocall_ic_hideme" android:tint="@color/sec_videocall_button_dim_tint" />
    </item>
    <item android:state_checked="true">
        <bitmap android:src="@drawable/videocall_ic_hideme" />
    </item>
    <item android:state_enabled="false" android:state_checked="false">
        <bitmap android:src="@drawable/videocall_ic_showme" android:tint="@color/sec_videocall_button_dim_tint" />
    </item>
    <item android:state_checked="false">
        <bitmap android:src="@drawable/videocall_ic_showme" />
    </item>
</selector>