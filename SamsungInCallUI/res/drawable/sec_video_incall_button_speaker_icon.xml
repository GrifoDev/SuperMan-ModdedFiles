<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/videocall_ic_speaker_on" android:tint="@color/sec_video_vga_button_on_dim_tint" />
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <bitmap android:src="@drawable/videocall_ic_speaker_on" android:tint="@color/sec_video_vga_button_on_tint" />
    </item>
    <item android:state_enabled="true" android:state_checked="false">
        <bitmap android:src="@drawable/videocall_ic_speaker" android:tint="@color/sec_video_vga_button_off_tint" />
    </item>
    <item android:state_enabled="false" android:state_checked="false">
        <bitmap android:src="@drawable/videocall_ic_speaker" android:tint="@color/sec_video_vga_button_dim_tint" />
    </item>
    <item>
        <bitmap android:src="@drawable/videocall_ic_speaker" />
    </item>
</selector>