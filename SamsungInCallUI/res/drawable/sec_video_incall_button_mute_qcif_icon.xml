<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/videocall_ic_mute_on" android:tint="@color/sec_video_call_button_text_dim_color" />
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <bitmap android:src="@drawable/videocall_ic_mute_on" />
    </item>
    <item android:state_enabled="true" android:state_checked="false">
        <bitmap android:src="@drawable/videocall_ic_mute" />
    </item>
    <item android:state_enabled="false" android:state_checked="false">
        <bitmap android:src="@drawable/videocall_ic_mute" android:tint="@color/sec_video_call_button_text_dim_color" />
    </item>
</selector>