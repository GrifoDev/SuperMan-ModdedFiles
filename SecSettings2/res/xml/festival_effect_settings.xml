    <Preference android:key="festival_effect_help" />
    <PreferenceCategory android:title="@string/allowed_functions" android:key="allowed_functions_category" />
    <com.android.settings.SwitchPreferenceScreen android:title="@string/home_lock_screen_title" android:key="home_lock_screen" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
        <extra android:name="type" android:value="home_lock_screen" />
    </com.android.settings.SwitchPreferenceScreen>
    <com.android.settings.SwitchPreferenceScreen android:title="@string/notification_panel_title" android:key="notification_panel" android:fragment="com.android.settings.festivaleffect.FestivalEffectGuide">
