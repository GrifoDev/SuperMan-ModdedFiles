        <TextView android:textSize="@dimen/fingerprint_register_v2_guideText_textSize" android:textColor="#ff313131" android:gravity="center_horizontal" android:id="@id/register_text_second" android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_marginTop="@dimen/fingerprint_register_firstGuide_marginTop" android:text="@string/fingerprint_rotate_guide_msg" android:lineSpacingExtra="@dimen/fingerprint_register_textLineSpace" android:fontFamily="sans-serif" android:layout_marginStart="@dimen/fingerprint_register_firstGuide_marginStart" android:layout_marginEnd="@dimen/fingerprint_register_firstGuide_marginStart" />
