        <level id="2" temp="450" />
        <level id="3" temp="470" />
        <level id="4" temp="490" />
        <level id="5" temp="510" />
        <level id="6" temp="530" />
    </levels>

    <default_scenario>
        <limiter name="CPUFreqMax" type="number">
            <table_item temp="430" value="-1" />
            <table_item temp="450" value="1378000" />
            <table_item temp="470" value="1274000" />
            <table_item temp="490" value="1144000" />
            <table_item temp="510" value="1040000" />
