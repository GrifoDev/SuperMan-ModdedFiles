        </limiter>
        <limiter name="Charging" type="number">
            <table_item temp="460" value="70" />
            <table_item temp="480" value="10" />
            <table_item temp="500" value="10" />
            <table_item temp="520" value="0" />
        </limiter>
        <limiter name="ChargingEvent" type="number">
