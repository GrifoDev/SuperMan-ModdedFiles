    <default_scenario>
        <limiter name="CPUFreqMax" type="number">
            <table_item temp="410" value="-1" />
            <table_item temp="430" value="1352000" />
            <table_item temp="450" value="1144000" />
            <table_item temp="470" value="936000" />
        </limiter>
        <limiter name="GPUFreqMax" type="number">
