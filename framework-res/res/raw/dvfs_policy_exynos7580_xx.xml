<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE DvfsHintList [
<!ELEMENT DvfsHintList (Hint?,TspHint+)>
<!ELEMENT Hint EMPTY>
<!ELEMENT TspHint EMPTY>
<!ATTLIST TspHint
          scenario_name CDATA #REQUIRED
          level CDATA #REQUIRED
          head CDATA #IMPLIED
          tail CDATA #IMPLIED>
]>

<DvfsHintList>
    <Resolution resoltype="Default">
        <TspHint scenario_name="Default"            level="2" head="200 1200000 1200000 0 0 0"  tail="300 1000000 1000000 0 0 0" />
        <TspHint scenario_name="SipDefault"         level="1" head="200 1200000 1200000 0 0 0"  tail="0 0 0 0 0 0" />

        <Hint name="AMS_RESUME" timeout="-1">
            <Control type="cpu_min" value="1500000"/>
        </Hint>

        <Hint name="AMS_RESUME_TAIL" timeout="600">
            <Control type="cpu_min" value="1500000"/>
        </Hint>

        <Hint name="AMS_RELAUNCH_RESUME" timeout="500">
            <Control type="cpu_min" value="1400000"/>
        </Hint>

        <Hint name="AMS_ACT_RESUME" timeout="-1">
            <Control type="cpu_min" value="1400000"/>
        </Hint>

        <Hint name="AMS_ACT_START" timeout="-1">
            <Control type="cpu_min" value="1500000"/>
        </Hint>

        <Hint name="GALLERY_TOUCH" timeout="500">
            <Control type="cpu_min" value="1100000"/>
        </Hint>

        <Hint name="GALLERY_SCROLL" timeout="-1">
            <Control type="cpu_min" value="1000000"/>
        </Hint>

        <Hint name="LAUNCHER_TOUCH" timeout="1000">
            <Control type="cpu_min" value="1100000"/>
        </Hint>

        <Hint name="PWM_ROTATION" timeout="1500">
            <Control type="cpu_min" value="max"/>
        </Hint>

        <Hint name="LISTVIEW_SCROLL" timeout="-1">
            <Control type="cpu_min" value="1000000"/>
        </Hint>

        <Hint name="DEVICE_WAKEUP" timeout="1000">
            <Control type="cpu_min" value="1100000"/>
        </Hint>

        <Hint name="APP_LAUNCH" timeout="2000">
            <Control type="cpu_min" value="max"/>
        </Hint>

        <Hint name="BADGE_UPDATE" timeout="2000">
            <Control type="cpu_min" value="1400000"/>
        </Hint>
    </Resolution>

    <Resolution resoltype="WQHD">
    </Resolution>

    <Resolution resoltype="FHD">
    </Resolution>

    <Resolution resoltype="HD">
        <TspHint scenario_name="Default"            level="2" head="200 1000000 1000000 0 0 0"  tail="300 800000 800000 0 0 0" />
        <TspHint scenario_name="SipDefault"         level="1" head="200 1000000 1000000 0 0 0"  tail="0 0 0 0 0 0" />

        <Hint name="AMS_RESUME" timeout="-1">
            <Control type="cpu_min" value="1300000"/>
        </Hint>

        <Hint name="AMS_RESUME_TAIL" timeout="600">
            <Control type="cpu_min" value="1300000"/>
        </Hint>

        <Hint name="AMS_RELAUNCH_RESUME" timeout="500">
            <Control type="cpu_min" value="1300000"/>
        </Hint>

        <Hint name="AMS_ACT_RESUME" timeout="-1">
            <Control type="cpu_min" value="1300000"/>
        </Hint>

        <Hint name="AMS_ACT_START" timeout="-1">
            <Control type="cpu_min" value="1400000"/>
        </Hint>

        <Hint name="GALLERY_TOUCH" timeout="500">
            <Control type="cpu_min" value="1000000"/>
        </Hint>

        <Hint name="GALLERY_SCROLL" timeout="-1">
            <Control type="cpu_min" value="900000"/>
        </Hint>

        <Hint name="LAUNCHER_TOUCH" timeout="1000">
            <Control type="cpu_min" value="1000000"/>
        </Hint>

        <Hint name="PWM_ROTATION" timeout="1500">
            <Control type="cpu_min" value="max"/>
        </Hint>

        <Hint name="LISTVIEW_SCROLL" timeout="-1">
            <Control type="cpu_min" value="900000"/>
        </Hint>

        <Hint name="DEVICE_WAKEUP" timeout="1000">
            <Control type="cpu_min" value="1000000"/>
        </Hint>

        <Hint name="APP_LAUNCH" timeout="2000">
            <Control type="cpu_min" value="max"/>
        </Hint>

        <Hint name="BADGE_UPDATE" timeout="2000">
            <Control type="cpu_min" value="1200000"/>
        </Hint>
    </Resolution>
</DvfsHintList>
