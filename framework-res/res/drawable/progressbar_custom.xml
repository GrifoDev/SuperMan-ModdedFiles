        <shape android:tint="#88ffffff" android:shape="rectangle">
            <size android:height="5.0dip" />
        </shape>
    </item>
    <item android:id="@id/progress">
        <scale android:scaleWidth="100%">
            <shape android:tint="@color/tw_progress_color_control_normal" android:shape="rectangle">
                <size android:height="5.0dip" />
            </shape>
        </scale>
    </item>
</layer-list>