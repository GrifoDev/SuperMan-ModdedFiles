<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center" android:id="@id/background">
        <shape android:tint="?^attr-private/semSeekBarNormalColor" android:shape="rectangle">
            <size android:height="@dimen/tw_seekbar_track_height_material" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:tint="@color/tw_seekbar_color_control_secondary" android:shape="rectangle">
                <size android:height="@dimen/tw_seekbar_track_height_material" />
            </shape>
        </scale>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/progress">
        <scale android:scaleWidth="100%">
            <shape android:tint="?^attr-private/semSeekBarActivatedColor" android:shape="rectangle">
                <size android:height="@dimen/tw_seekbar_track_height_material" />
            </shape>
        </scale>
    </item>
</layer-list>