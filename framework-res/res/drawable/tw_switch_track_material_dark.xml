<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <inset android:drawable="@drawable/tw_switch_track_mtrl_dark_alpha_disabled" />
    </item>
    <item android:state_enabled="false">
        <inset android:drawable="@drawable/tw_switch_track_mtrl_dark_alpha_disabled" />
    </item>
    <item android:state_checked="true">
        <inset android:drawable="@drawable/tw_switch_track_mtrl_dark_alpha_on" />
    </item>
    <item>
        <inset android:drawable="@drawable/tw_switch_track_mtrl_dark_alpha_off" />
    </item>
</selector>