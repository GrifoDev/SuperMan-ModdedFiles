<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center" android:id="@id/background">
        <shape android:tint="?^attr-private/semProgressNormalColor" android:shape="rectangle">
            <size android:height="@dimen/tw_progress_bar_height_material" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:tint="?^attr-private/semProgressActivatedColor" android:shape="rectangle">
                <size android:height="@dimen/tw_progress_bar_height_material" />
            </shape>
        </scale>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/progress">
        <scale android:scaleWidth="100%">
            <shape android:tint="?^attr-private/semProgressActivatedColor" android:shape="rectangle">
                <size android:height="@dimen/tw_progress_bar_height_material" />
            </shape>
        </scale>
    </item>
</layer-list>