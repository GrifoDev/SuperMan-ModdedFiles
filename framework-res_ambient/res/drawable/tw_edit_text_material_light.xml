<?xml version="1.0" encoding="utf-8"?>
<inset android:inset="0.0dip"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_enabled="false">
            <nine-patch android:src="@drawable/tw_textfield_default_mtrl_alpha" android:tint="@color/tw_edit_text_bg_tint_color_enabled_false_material_light" />
        </item>
        <item android:state_focused="true">
            <nine-patch android:src="@drawable/tw_textfield_activated_mtrl_alpha" android:tint="?colorPrimary" />
        </item>
        <item android:state_pressed="true">
            <nine-patch android:src="@drawable/tw_textfield_activated_mtrl_alpha" android:tint="?colorPrimary" />
        </item>
        <item>
            <nine-patch android:src="@drawable/tw_textfield_default_mtrl_alpha" android:tint="@color/tw_edit_text_bg_tint_color_focused_false_material_light" />
        </item>
    </selector>
</inset>