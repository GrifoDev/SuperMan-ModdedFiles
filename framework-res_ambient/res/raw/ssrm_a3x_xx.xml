<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE siop_document [
<!ELEMENT siop_document (levels , default_scenario , extra_scenario?)>
<!ELEMENT levels (level)+>
<!ELEMENT level EMPTY>
<!ATTLIST level
          id CDATA #REQUIRED
          temp CDATA #REQUIRED>
<!ELEMENT default_scenario (limiter)+>
<!ELEMENT limiter (sts_item* , table_item*)>
<!ATTLIST limiter
          name CDATA #REQUIRED
          type (STS|number|bool) #REQUIRED>
<!ELEMENT sts_item EMPTY>
<!ATTLIST sts_item
          pre CDATA #IMPLIED
          perc CDATA #IMPLIED
          trigger CDATA #REQUIRED
          release CDATA #REQUIRED
          min CDATA #IMPLIED
          max CDATA #IMPLIED
          duration CDATA #IMPLIED>
<!ELEMENT table_item EMPTY>
<!ATTLIST table_item
          temp CDATA #REQUIRED
          value CDATA #REQUIRED
          low_temp CDATA #IMPLIED>
<!ELEMENT extra_scenario (scenario)+>
<!ELEMENT scenario (trigger_statuses? , trigger_packages? , limiter*)>
<!ATTLIST scenario name CDATA #REQUIRED>
<!ATTLIST scenario priority CDATA #IMPLIED>
<!ELEMENT trigger_statuses (status)+>
<!ELEMENT status EMPTY>
<!ATTLIST status name CDATA #REQUIRED>
<!ELEMENT trigger_packages (package)+>
<!ELEMENT package EMPTY>
<!ATTLIST package name CDATA #REQUIRED>
] >

<siop_document>

    <levels>
        <level id="0" temp="440"/>
        <level id="1" temp="460"/>
        <level id="2" temp="480"/>
        <level id="3" temp="500"/>
        <level id="4" temp="520"/>
        <level id="5" temp="540"/>
    </levels>

    <default_scenario>
        <limiter name="CPUFreqMax" type="number">
            <table_item temp="460" value="-1" />
            <table_item temp="480" value="1400000" />
            <table_item temp="500" value="1200000" />
            <table_item temp="520" value="1000000" />
            <table_item temp="540" value="800000" />
        </limiter>
        <limiter name="GPUFreqMax" type="number">
            <table_item temp="460" value="-1" />
            <table_item temp="480" value="534" />
            <table_item temp="500" value="440" />
            <table_item temp="520" value="350" />
            <table_item temp="540" value="266" />
        </limiter>
        <limiter name="PST" type="number">
            <table_item temp="440" value="1" />
        </limiter>
        <limiter name="LCDBrightness" type="number">
            <table_item temp="480" value="-1" />
            <table_item temp="500" value="230" />
        </limiter>
        <limiter name="Charging" type="number">
            <table_item temp="460" value="100" />
            <table_item temp="480" value="70" />
            <table_item temp="500" value="50" />
            <table_item temp="520" value="30" />
            <table_item temp="540" value="0" />
        </limiter>
        <limiter name="Flash" type="bool">
            <table_item temp="540" value="0" />
        </limiter>
        <limiter name="SmartBonding" type="bool">
            <table_item temp="500" value="0" />
        </limiter>
    </default_scenario>

    <extra_scenario>
        <scenario name="ScreenOff" priority="1">
            <limiter name="Charging" type="number">
                <table_item temp="440" value="100" />
            </limiter>
        </scenario>

        <scenario name="ScreenOffInCall" priority="1">
            <limiter name="Charging" type="number">
                <table_item temp="440" value="50" />
            </limiter>
        </scenario>

        <scenario name="Hangouts">
            <trigger_packages>
                <package name="com.google.android.talk" />
            </trigger_packages>

            <limiter name="GPUFreqMax" type="number">
                <table_item temp="440" value="440" />
            </limiter>
        </scenario>

        <scenario name="HangoutsVTCall">
            <limiter name="CPUFreqMax" type="number">
                <table_item temp="440" value="1000000" />
            </limiter>
        </scenario>

        <scenario name="BrowserMode">
            <trigger_packages>
               <package name="com.sec.android.app.sbrowser" />
               <package name="com.android.browser" />
               <package name="com.android.chrome" />
            </trigger_packages>

             <limiter name="Charging" type="number">
                <table_item temp="440" value="50" />
            </limiter>
        </scenario>

        <scenario name="HotGameControl3">
            <trigger_packages>
                <package name="com.ea.games.r3_row" />
                <package name="com.ea.games.r3_na" />
            </trigger_packages>

            <limiter name="PST" type="number">
                <table_item temp="460" value="0" />
            </limiter>
            <limiter name="CPUFreqMax" type="number">
                <table_item temp="400" value="-1" />
                <table_item temp="440" value="1000000" />
                <table_item temp="480" value="1000000" />
                <table_item temp="500" value="900000" />
            </limiter>
            <limiter name="GPUFreqMax" type="number">
                <table_item temp="400" value="-1" />
                <table_item temp="440" value="440" />
                <table_item temp="460" value="350" />
            </limiter>
           <limiter name="Charging" type="number">
                 <table_item temp="440" value="50" />
            </limiter>
        </scenario>

        <scenario name="HotGameControlExtreme2">
            <trigger_packages>
                <package name="com.ftt.soulblade_gl_4kakao" />
                <package name="com.ftt.soulblade_t_4kakao" />
                <package name="com.sq.dragonsworld.kakao" />
                <package name="com.aim.racing" />
                <package name="com.wb.goog.mkx" />
                <package name="com.ludia.jurassicworld" />
                <package name="com.mgc.miami.crime.simulator" />
            </trigger_packages>

            <limiter name="PST" type="number">
                <table_item temp="440" value="0" />
            </limiter>
            <limiter name="CPUFreqMax" type="number">
                <table_item temp="400" value="-1" />
                <table_item temp="440" value="1000000" />
            </limiter>
            <limiter name="GPUFreqMax" type="number">
                <table_item temp="400" value="-1" />
                <table_item temp="440" value="440" />
                <table_item temp="460" value="350" />
            </limiter>
           <limiter name="Charging" type="number">
                 <table_item temp="440" value="50" />
            </limiter>
        </scenario>

        <scenario name="AppStart" priority="1">
            <limiter name="CPUFreqMax" type="number">
                <table_item temp="460" value="-1" />
            </limiter>
            <limiter name="GPUFreqMax" type="number">
                <table_item temp="460" value="-1" />
            </limiter>
        </scenario>

        <scenario name="LowTemp" priority="2">
            <limiter name="PST" type="number">
                <table_item temp="440" value="2" />
            </limiter>
            <limiter name="CPUFreqMax" type="number">
                <table_item temp="-150" value="800000" />
                <table_item temp="-140" value="-1" />
            </limiter>
            <limiter name="GPUFreqMax" type="number">
                <table_item temp="-150" value="266" />
                <table_item temp="-140" value="-1" />
            </limiter>
            <limiter name="LCDBrightness" type="number">
                <table_item temp="-150" value="180" />
                <table_item temp="-140" value="-1" />
            </limiter>
        </scenario>
    </extra_scenario>

</siop_document>
